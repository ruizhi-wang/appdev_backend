<h1>New listing</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@listing) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :location %>
      <%= f.text_field :location, class: 'form-control' %>

      <%= f.submit "Create listing", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
